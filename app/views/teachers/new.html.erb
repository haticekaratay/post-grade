<div class="text-center">
    <%= form_for @teacher do |f| %>
        <%= f.label :username%><br>
        <%= f.text_field :username%><br>

        <%= f.label :email %><br>
        <%= f.text_field :email%><br>

        <%= f.label :password %><br>
        <%= f.password_field :password%><br>

        <%= f.submit 'Signup' %>
    <% end %>
