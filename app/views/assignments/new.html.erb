<%= form_for @assignment do |f| %>
    <%= f.label :"Assignment Name" %>
    <%= f.text_field  :name%>
    
    <%= f.label :description %>
    <%= f.text_area :description%>

    <%= f.label "Total" %>
    <%= f.number_field  :max%>

    <%= f.label :due_date %>
    <%= f.date_field  :due_date%>

    <%= f.submit %>
<% end %>